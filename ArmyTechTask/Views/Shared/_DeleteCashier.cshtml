@model Cashier

<form asp-action="DeleteNew" asp-controller="Cashier" data-ajax="true" data-ajax-success="closeModal()" data-ajax-update="#TR_@Model.Id">
	<input asp-for="Id" hidden />
	<div class="border p-3 mt-4">
		<div class="row pb-2">
			<h2 class="text-primary">Are you Sure To Delete This Cashier?</h2>
			<hr />
		</div>
		<div class="mb-3">
			<label asp-for="Id"></label>
			<input asp-for="Id" disabled class="form-control" />
		</div>
		<div class="mb-3">
			<label asp-for="CashierName"></label>
			<input asp-for="CashierName" disabled class="form-control" />
		</div>
		<div class="mb-3">
			<label asp-for="Branch.BranchName"></label>
			<input asp-for="Branch.BranchName" disabled class="form-control" />
		</div>

		<button type="submit" class="btn btn-danger" style="width:150px">Delete</button>
		<a onclick="closeModal();" class="btn btn-secondary" style="width:150px">
			Back to List
		</a>
	</div>
</form>

@section Scripts{
	@{
	<partial name="_ValidationScriptsPartial" />
	}
}
